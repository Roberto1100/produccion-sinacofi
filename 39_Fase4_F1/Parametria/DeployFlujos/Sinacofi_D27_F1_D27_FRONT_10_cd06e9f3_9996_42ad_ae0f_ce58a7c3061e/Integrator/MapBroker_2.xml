         <DecisionPath>
            <StepMappingRule>
               <MappingRule id="1cb16b97-c668-4742-bf9c-e7da78efdc15" label="" lastModified="2024-08-13T16:15:23.000Z" name="MAPSERVICE_Sinacofi_D27_F1_D27_FRONT::MAPSERVICE.Sinacofi_D27_F1.Calcular_Campos:Calcular_Campos" xmlVersion="1.0">
                  <OutputBusinessDocument id="29b6fc93-a3f5-4ed1-bd2a-a3828141c0b1">
                     <name>MAPSERVICE_Sinacofi_D27_F1_D27_FRONT::MAPSERVICE.Sinacofi_D27_F1\Salida.D27_Caratula_Salida</name>
                     <lastModified>2024-08-13T16:15:23.000Z</lastModified>
                  </OutputBusinessDocument>
                  <MappingElement ElementReferenceId="2">
                     <Expression>
                        <DmlExpression><![CDATA[getSubString( \Header\Datos, 7,8 )]]></DmlExpression>
                     </Expression>
                     <Comment/>
                  </MappingElement>
                  <MappingElement ElementReferenceId="3">
                     <Expression>
                        <DmlExpression><![CDATA[count(\Details)]]></DmlExpression>
                     </Expression>
                     <Comment/>
                  </MappingElement>
                  <MappingElement ElementReferenceId="5">
                     <Expression>
                        <DmlExpression><![CDATA[%Total_AJL]]></DmlExpression>
                     </Expression>
                     <Comment/>
                  </MappingElement>
                  <MappingElement ElementReferenceId="4">
                     <Expression>
                        <DmlExpression><![CDATA[%Total_AJL:=0;
%Total_AJK:=0;
loop on \Details  {
	if( getSubString( this\Contenido, 62,1) = "0" )
	then{
		%Total_AJK:= %Total_AJK + convertToI(getSubString(this\Contenido, 63, 14))
	};
	if( getSubString( this\Contenido, 62,1) in ( "1","2","3","4","5","6","7","8","9") and getSubString( this\Contenido, 61,1 ) in ( "0","1","8","9" ) )
	then{
		%Total_AJL:= %Total_AJL + convertToI(getSubString(this\Contenido, 63, 14))
	}
};
%Total_AJK]]></DmlExpression>
                     </Expression>
                     <Comment/>
                  </MappingElement>
               </MappingRule>
               <StepOutput>
                  <OutputName>out</OutputName>
               </StepOutput>
            </StepMappingRule>
            <StepOutput>
               <OutputName>out1</OutputName>
               <InputBusinessDocumentAlias>in</InputBusinessDocumentAlias>
            </StepOutput>
            <StepOutput>
               <OutputName>out2</OutputName>
               <InputBusinessDocumentAlias>in</InputBusinessDocumentAlias>
            </StepOutput>
            <StepStep>
               <DmlExpression><![CDATA[%correlativoPrefijo:= getSubString(getAttribute( "B2BXGIReceive_ConsumptionFilename" ), 5, 8 );
setDocumentAttributeToOutput( "out1",  "B2BXGISend_ProductionFilename",%correlativoPrefijo + "_" + "datosSalida");
setDocumentAttributeToOutput( "out2",  "B2BXGISend_ProductionFilename", getSubString(getAttribute("B2BXGIReceive_ConsumptionFilename" ), 5, 255) + getAttribute( "B2BXGIReceive_ConsumptionExtension"))]]></DmlExpression>
