         <InputBusinessDocuments>
            <InputBusinessDocument>
               <BusinessDocument id="5a26c413-5426-4534-8607-9d23965d6be1">
                  <name>MAPSERVICE_Sinacofi_P20_F1_P20_FRONT::MAPSERVICE.Sinacofi_P20_F1\ArchivosEntrada.P20_Archivo_Datos</name>
